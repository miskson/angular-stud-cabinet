{
  "name": "cabinet",
  "version": "0.0.0",
  "homepage": "https://miskson.github.io/angular-stud-cabinet",
  "scripts": {
    "ng": "ng",
    "save-json-api": "node ./src/app/server/scripts/save-json-db-api.js",
    "start": "ng serve",
    "build": "ng build --base-href=/angular-stud-cabinet && npm run save-json-api",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "predeploy": "rm -rf docs && npm run build",
    "deploy": "gh-pages -d build",
    "init-db": "json-server --watch ./src/DB/db.json",
    "demo": "npm run init-db & ng serve"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^22.6.1",
    "gh-pages": "^6.1.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "node-fs": "^0.1.7",
    "typescript": "~4.9.4"
  }
}
